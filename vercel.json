{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/public/images/$1"
    },
    {
      "source": "/certificates/(.*)",
      "destination": "/public/certificates/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "public": true
}